{
	"rust-analyzer.serverPath": "/usr/local/bin/rust-analyzer",
	"rust-analyzer.cargo.allFeatures": true,
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": [
				"go.mod",
				".vim/",
				".git/",
				".hg/"
			],
			"filetypes": [
				"go"
			],
			"initializationOptions": {
				"usePlaceholders": true
			}
		}
	}
}
